{
  "name": "cms",
  "version": "0.1.0",
  "license": "MIT",
  "scripts": {
    "ng": "ng",
    "build": "npm run build:cms && npm run build:client",
    "build:cms": "ng build --app=cms",
    "build:client": "ng build --app=client",
    "build:prod": "npm run build:prod-cms && npm run build:prod-client",
    "build:prod-cms": "ng build --prod --aot --output-hashing none --app=cms",
    "build:prod-client": "ng build --prod --aot --output-hashing none --app=client",
    "test:ng": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "dev:client": "ng build --watch",
    "dev:server": "NODE_ENV=dev nodemon --watch 'src/server/**/*.ts' --ignore 'src/**/*.spec.ts' --exec 'ts-node' src/server/server.ts",
    "prod:server": "NODE_ENV=production nodemon --watch 'src/server/**/*.ts' --ignore 'src/**/*.spec.ts' --exec 'ts-node' src/server/server.ts",
    "dev:pug": "pug -w views -o build/views",
    "init:dev": "NODE_ENV=dev 'ts-node' src/server/init/init.ts",
    "init:prod": "NODE_ENV=production 'ts-node' src/server/init/init.ts",
    "test": "NODE_ENV=test mocha -r ts-node/register src/**/*-test.spec.ts"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^4.2.3",
    "@angular/common": "^4.2.3",
    "@angular/compiler": "^4.2.3",
    "@angular/core": "^4.2.3",
    "@angular/forms": "^4.2.3",
    "@angular/http": "^4.2.3",
    "@angular/material": "=2.0.0-beta.6",
    "@angular/platform-browser": "^4.2.3",
    "@angular/platform-browser-dynamic": "^4.2.3",
    "@angular/router": "^4.2.3",
    "@swimlane/ngx-datatable": "^7.3.0",
    "@types/lodash": "^4.14.64",
    "@types/moment": "^2.13.0",
    "@types/node": "^6.0.68",
    "@types/socket.io": "^1.4.29",
    "@types/socket.io-client": "^1.4.29",
    "angular2-material-datepicker": "^0.5.0",
    "angular2-notifications": "^0.4.53",
    "angular2-recaptcha": "^0.6.0",
    "app-root-dir": "^1.0.2",
    "async-busboy": "^0.4.0",
    "autobahn": "^17.5.2",
    "chalk": "^1.1.3",
    "chart.js": "^2.6.0",
